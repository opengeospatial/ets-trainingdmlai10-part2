{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/md_format.json",
    "title": "MD_Format",
    "type": "object",
    "required": [
        "formatSpecificationCitation"
    ],
    "properties": {
        "formatSpecificationCitation": {
            "$ref": "./ci_citation.json"
        },
        "amendmentNumber": {
            "type": "string"
        },
        "fileDecompressionTechnique": {
            "type": "string"
        },
        "medium": {
            "type": "array",
            "items": {
                "$ref": "./md_medium.json"
            }
        },
        "formatDistributor": {
            "type": "array",
            "items": {
                "$ref": "./md_distributor.json"
            }
        }
    }
}