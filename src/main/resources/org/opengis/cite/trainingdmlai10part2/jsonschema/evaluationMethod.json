{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/evaluationMethod.json",
    "title": "EvaluationMethod",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "evaluationMethodDescription": {
            "type": "string"
        },
        "evaluationMethodType": {
            "$ref": "./evaluationMethodTypeCode.json"
        },
        "evaluationProcedure": {
            "$ref": "./ci_citation.json"
        },
        "dateTime": {
            "type": "array",
            "items": {
                "$ref": "./dateTime.json"
            }
        },
        "referenceDoc": {
            "type": "array",
            "items": {
                "$ref": "./ci_citation.json"
            }
        },
        "deductiveSource": {
            "type": "string"
        }
    }
}