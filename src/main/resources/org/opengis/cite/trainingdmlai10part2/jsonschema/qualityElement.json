{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:8000/qualityElement.json",
    "title": "QualityElement",
    "type": "object",
    "required": [
        "type",
        "measure",
        "evaluationMethod",
        "result"
    ],
    "properties": {
        "type": {
            "type": "string"
        },
        "measure": {
            "$ref": "./measureReference.json"
        },
        "evaluationMethod": {
            "$ref": "./evaluationMethod.json"
        },
        "result": {
            "type": "array",
            "items": {
                "$ref": "./qualityResult.json"
            },
            "minItems": 1
        }
    }
}